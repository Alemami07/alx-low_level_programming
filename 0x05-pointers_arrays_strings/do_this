#include "main.h"
#include <string.h>

/**
 *_strlen - Entry point
 *Return: Always 0 (success)
 *@str: return value
*/

size_t _strlen(const char *s)
{
        int num;
        int len;
        char *str;

        len = strlen(str);

        num = 0;

        while
                (*s != '\0');
        {
                num++;
        }
                *s++;

        return (num);
}
